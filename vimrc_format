set encoding=utf8
set paste
set expandtab
set textwidth=0
set tabstop=4
set shiftwidth=4
set autoindent

”set increase search, which will search immediately when type
’set increase search, which will search immediately when type
set incsearch   


set ignorecase
set ruler
set wildmenu
set commentstring=\ #\ %s
set foldlevel=0
syntax on